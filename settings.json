                "descending": false,
                "infinite": false,
                "pageSize": 30,
                "sort": "name"
              }
            },
            "version": "0.0.6"
          },
          {
            "component": "tws-article-list",
            "settings": {
              "controls": {
                "availableSort": [
                  "name",
                  "price"
                ],
                "hide": {
                  "descending": false,
                  "infinite": true,
                  "pageSize": false,
                  "pagination": false,
                  "sort": false,
                  "type": false
                },
                "pageSize": [],
                "type": "grid"
              },
              "defaults": {
                "descending": false,
                "infinite": true,
                "pageSize": 30,
                "sort": "name"
              }
            },
            "version": "0.1.11"
