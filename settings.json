  "custom_css": {
    "rules": "@import url(http://fonts.googleapis.com/css?family=Lobster);\n\nheader {\n   font-family: 'Lobster', cursive;\n}\n\nheader h1 {\n  font-size: 62px;\n}\nh1,h2,h3,h4 {\ntext-shadow: 1px 1px 1px #FFFFFF;\n  font-family: 'Lobster', cursive;\n}\n\nfooter {\n text-align: center;\n}\n\n.yellow {\ncolor: yellow\n}\n\ntws-articlegroup-menu > ul {\n   list-style-type: none;\n}\n\ntws-articlegroup-menu > ul > li {\n   list-style-type: none;\n}\n\ntws-articlegroup-menu > ul > li > ul {\n   list-style-type: none;\n}\n\ntws-articlegroup-menu > ul > li > ul > li {\n   list-style-type: none;\n}\n\n\nhtml, body {\n  padding: 0;\n  margin: 0;\n  height: 100%;\n  background: #f2f2f2;\n  color: #333;\n  font-family: 'Segoe UI', 'Helvetica neue', Arial;\ntext-shadow: 1px 1px 1px #FFFFFF;\n}\n\n.tws-theme > header {\n  padding: 15px;\n  border-bottom: 1px solid #cecece;\n}\n\n.tws-theme .left-pane  ul {\npadding-left: 15px;\n}\n\n.tws-theme {\n}\n\n.tws-theme  a {\n}\n\n.tws-theme > div > .left-pane {\n  border-right: 1px solid #cecece;\n}\n\n.tws-theme > div > .content {\n}\n\n.tws-theme > div > .right-pane {\n  border-left: 1px solid #cecece;\n}\n\n.left-pane {\n  padding: 5px;\n\n}\n\n\n/*\n  FIXME: We need to decide on how we will handle height in the layout.\n  This is extra problematic since we optimally would like footer and header to grow with\n  whatever you put into them and at the same time take up full page height at a minimum.\n  And like side panes to have same height as the columns etc.\n*/\n.tws-theme > div > .content {\n  min-height: 800px;\n}",
    "links": []
  }
}
