      "ArticleSearchResults": {
        "hide_left_pane": false,
        "hide_right_pane": false
      },
      "Articlegroup": {
        "hide_left_pane": true,
        "hide_right_pane": true,
        "left_pane": [],
        "middle": [
          {
            "component": "tws-articlegroup-list",
            "settings": {
              "controls": {
                "availableSort": [
                  "name",
                  "price",
                  "articleNumber",
                  "weight",
                  "ean",
                  "sku"
